model Video {
  id            String   @id @default(uuid())
  name          String
  path          String
  transcription String?
  createdAt     DateTime @default(now())

  // @@map("video")
}

model Prompt {
  id       String @id @default(uuid())
  title    String
  template String

  // @@map("prompt")
}

